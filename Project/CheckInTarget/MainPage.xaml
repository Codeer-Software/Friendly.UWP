<Page
    x:Class="CheckInTarget.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:CheckInTarget"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" Width="712.425" Height="714.242">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Button x:Name="button1" Content="{Binding Content1}" HorizontalAlignment="Left" Margin="39,62,0,0" VerticalAlignment="Top" Click="button1_Click"/>
        <Button x:Name="button2" Content="{Binding Value2}" HorizontalAlignment="Left" Margin="39,139,0,0" VerticalAlignment="Top" Click="button2_Click"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Margin="39,219,0,0" TextWrapping="Wrap" Text="{Binding Text1}" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Margin="39,301,0,0" TextWrapping="Wrap" Text="{Binding Value2}" VerticalAlignment="Top"/>
        <CheckBox x:Name="checkBox" Content="CheckBox" HorizontalAlignment="Left" Margin="39,377,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="39,437,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Page>
